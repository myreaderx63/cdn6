<script type="text/javascript" src="/wro/j9bp~product.js"></script>
<script>
    if ($('.seamlessAccessDenial').length) {
        window.addEventListener('load', function () {
            var $seamlessAccess = $('.seamlessAccessDenial'),
                $header = $seamlessAccess.find('h5'),
                $icon = $seamlessAccess.find('.icon'),
                $text = $seamlessAccess.find('.text'),
                defaultHref = '/action/ssostart?redirectUri=' + encodeURIComponent(window.location.pathname + '?saml_referrer'),
                isAccessLoaded = false,
                entityAccess03 = [],
                entityAccess04 = [],
                entitydata03 = {},
                entitydata04 = {},
                seamlessAccessCode = $('.seamless-access-data').data('accesscode'),
                seamlessName = $('.seamless-access-data').data('name');


            setTimeout(function () {
                if (!isAccessLoaded) {
                    $text.append('<a class="temp-link" href="' + defaultHref + '">(or click to choose manually)</a>');
                }
            }, 5000);

            SeamlessAccessService.getEntitiesWithAllInfo(digitalData.publication.item.doi).then(function (res) {
                console.log(JSON.stringify(res));
                let i = res.length - 1;
                for (i; i >= 0; i--) {

                    var entityStatus = res[i];

                    switch (entityStatus.entityStatusObj.code) {
                        case 1003:
                            if (!entityAccess03.length > 0) {
                                entityAccess03.push(entityStatus.entityTitle, entityStatus.entitySsoUrl);
                                entitydata03 = {
                                    entityID: entityStatus.entityID,
                                    title: entityStatus.entityTitle
                                };
                            }
                            break

                        case 1004:
                            if (!entityAccess04.length > 0) {
                                entityAccess04.push(entityStatus.entityTitle, entityStatus.entitySsoUrl);
                                entitydata04 = {
                                    entityID: entityStatus.entityID,
                                    title: entityStatus.entityTitle
                                };
                            }
                            break

                    }
                }

                isAccessLoaded = true;


                $icon.html('<i aria-hidden="true" class="icon-account_balance"></i>');
                if (entityAccess03.length > 0) {

                    $text.html("<span>Access through</span> <b><a class= 'update-institution-entity'  data-updateEntity='" + JSON.stringify(entitydata03) + "' href='#' data-link='" + entityAccess03[1] + "'>" + entityAccess03[0] + "</a></b>");
                    $seamlessAccess.append('<div class="extra-login"><a href="' + defaultHref + '">or log in with a different institution</a></div>');
                } else if (entityAccess04.length > 0) {

                    $text.html("<span>Access through</span> <b><a class= 'update-institution-entity'  data-updateEntity='" + JSON.stringify(entitydata04) + "' href='#' data-link='" + entityAccess04[1] + "'>" + entityAccess04[0] + "</a></b>");
                    $seamlessAccess.append('<div class="extra-login"><a href="' + defaultHref + '">or log in with a different institution</a></div>');
                } else {
                    $text.html("<b><a href='" + defaultHref + "'>Access through your institution </a></b>");
                }


                if (seamlessAccessCode == 1002) {

                    $('<p class="institution-status">' + seamlessName + ' does not provide access to this content.</p>').insertAfter($header);
                } else if (seamlessAccessCode == 1004) {

                    $('<p class="institution-status">logged in as ' + seamlessName + '.</p>').insertAfter($header);
                }


            }, function (error) {
                console.log(error);
            });
        });
    }
    if ($('.recent-institution_list').length) {
        SeamlessAccessService.retrieveEntity().then(function (res) {
            var link, entitydata;
            let arrayOfPromises = [];
            for (let i in res) {
                link = '/pb/widgets/shibboleth/isSupportedInstitution?entityid=' + res[i].entity.entityID;
                arrayOfPromises.push(new Promise(function (resolve, reject) {
                    $.get(link, {})
                        .done(function (data, textStatus, jqXHR) {
                            if (typeof data === 'string') {
                                data = JSON.parse(data)
                            }
                            if (data.supported) {
                                resolve(res[i]);
                            }else {
                                reject();
                            }
                        })
                        .fail(function (jqXHR, textStatus, errorThrown) {
                            reject();
                            console.log('error occure on the server ' + errorThrown);
                        });
                }));
            }

            Promise.allSettled(arrayOfPromises)
                .then(function (finalResponse) {
                    let i = finalResponse.length - 1;
                    for(i; i >= 0; i--){
                        if(finalResponse[i].status === "fulfilled"){
                            let entity = finalResponse[i].value.entity;
                            entitydata = {
                                entityID: entity.entityID,
                                title: entity.title
                            };
                            if (!$('.recent-institution_list').parent().hasClass('card card--light-shadow')) {
                                $('.recent-institution_list').parent().addClass('card card--light-shadow');
                                $('.recent-institution_list').prepend("<div class='widget-header'> <h2>Recent institutions</h2></div>");
                            }
                            if (entity.entity_icon_url && entity.entity_icon_url.url) {

                                $('.recent-institution_list').append("<li class='recent-institution_item'><a class='update-institution-entity' data-updateEntity='" + JSON.stringify(entitydata) + "' href='#' data-link='" + SeamlessAccessService.getWAYFLessUrl(entity.entityID) + "'><img class='recent-institution_icon' src='" + entity.entity_icon_url.url + "' width='" + entity.entity_icon_url.width + "' height='" + entity.entity_icon_url.height + "'><span class='recent-institution_name'>" + entity.title + "</span> <i class='icon-arrow_r'> </i></a></li>");
                            } else {

                                $('.recent-institution_list').append("<li class='recent-institution_item'><a class='update-institution-entity' data-updateEntity='" + JSON.stringify(entitydata) + "' href='#' data-link='" + SeamlessAccessService.getWAYFLessUrl(entity.entityID) + "'><i class='recent-institution_icon icon-account_balance'></i><span class='recent-institution_name'>" + entity.title + "</span> <i class='icon-arrow_r'> </i></a></li>");
                            }
                        }
                    }
                });

        });
    }


    $(document).on('click', '.update-institution-entity , .searchInstitution .ms-res-item', function (e) {
        let $this = $(this);
        let updateInstEntity = $this.data('updateentity');

        SeamlessAccessService.updateEntity(updateInstEntity).then(function (res) {
            console.log('Entity Updated');

            let href = $this.data('link');
            if(href !== undefined){
                window.location.href = href;
            }
        });
    });

</script>
